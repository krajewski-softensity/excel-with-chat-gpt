@model IEnumerable<CrmToRecruit.Domain.ExcelData>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.RecordId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AccountName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DealName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DealOwner)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NumberOfResources)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.StageOfOpen)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.StageOfClosed)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.JobOpeningCreationDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LastActivityTime)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MustHaveSkills)

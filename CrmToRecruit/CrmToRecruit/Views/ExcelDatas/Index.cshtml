            <th>
                @Html.DisplayNameFor(model => model.ConfirmedST)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ConfirmedVendor)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RejectedST)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RejectedVendor)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.RecordId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AccountName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DealName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DealOwner)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NumberOfResources)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StageOfOpen)

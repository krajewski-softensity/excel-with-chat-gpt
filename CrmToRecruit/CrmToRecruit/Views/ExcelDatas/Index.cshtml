                @Html.DisplayFor(modelItem => item.ConfirmedVendor)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RejectedST)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RejectedVendor)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
